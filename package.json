{
  "name": "youtube-music-dl",
  "version": "1.0.0",
  "description": "Youtube Music DL - Youtube Music Downloader. Download music file with tags and cover.",
  "main": "webpack.config.js",
  "scripts": {
    "build": "npm run clean-dist && webpack --mode production",
    "clean-dist": "rm -rf dist/* dist/**/*",
    "build-watch": "webpack --watch --mode development",
    "lint": "eslint src/*.js src/**/*.js",
    "test": "mocha \"./src/**/*_test.js\" --require @babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougppaz/youtube-music-dl.git"
  },
  "keywords": [
    "youtube",
    "music",
    "download"
  ],
  "author": "Douglas Paz <oi@douglaspaz.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dougppaz/youtube-music-dl/issues"
  },
  "homepage": "https://github.com/dougppaz/youtube-music-dl#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "eslint": "^7.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "mocha": "^8.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "blob-stream": "^0.1.3",
    "btoa": "^1.2.1",
    "jDataView": "^2.3.0",
    "lodash-es": "^4.17.15",
    "mime-types": "^2.1.27",
    "vue": "^2.6.12",
    "ytdl-core": "^3.4.2"
  }
